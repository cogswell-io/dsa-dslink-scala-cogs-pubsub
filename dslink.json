{
  "name": "dslink-scala-cogswell-pubsub",
  "version": "1.0.0",
  "description": "Cogswell.io Pub/Sub DSLink",
  "license": "Apache",
  "author": {
    "name": "Aviata, Inc.",
    "email": "support@cogswell.io"
  },
  "main": "bin/dslink-scala-cogswell-pubsub",
  "repository": {
    "type": "git",
    "url": "https://github.com/cogswell-io/dsa-dslink-scala-cogs-pubsub"
  },
  "bugs": {
  	"url": "https://github.com/cogswell-io/dsa-dslink-scala-cogs-pubsub/issues"
  },
  "configs": {
    "name": {
      "type": "string",
      "default": "cogswell-pubsub"
    },
    "broker": {
      "type": "url"
    },
    "nodes": {
      "type": "path",
      "default": "nodes.json"
    },
    "key": {
      "type": "path",
      "default": ".key"
    },
    "log": {
      "type": "enum",
      "default": "info"
    },
    "handler_class": {
      "type": "string",
      "default": "io.cogswell.dslink.pubsub.Main$"
    }
  }
}